{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "4.0.3",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "yorkie": "2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.4.6",
    "@umijs/test": "3.0.12",
    "lint-staged": "10.0.8",
    "prettier": "2.0.1",
    "umi": "3.0.12",
    "umi-plugin-antd-icon-config": "2.0.0",
    "umi-types": "0.5.13"
  }
}

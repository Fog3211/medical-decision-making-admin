{
  "name": "frontend",
  "version": "0.0.1",
  "description": "基于飞冰搭建的数据可视化处理平台",
  "dependencies": {
    "@alifd/next": "1.19.12",
    "@icedesign/theme": "1.2.0",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "viser-react": "^2.4.8"
  },
  "devDependencies": {
    "@ice/spec": "0.1.9",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "css-modules-typescript-loader": "4.0.0",
    "eslint": "6.8.0",
    "ice-plugin-fusion": "0.1.12",
    "ice-plugin-moment-locales": "0.1.0",
    "ice-scripts": "2.1.16",
    "stylelint": "13.1.0"
  },
  "scripts": {
    "start": "ice-scripts dev",
    "build": "ice-scripts build",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx ./",
    "stylelint": "stylelint ./**/*.scss"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "iceworks": {
    "type": "react",
    "adapter": "adapter-react-v3"
  },
  "ideMode": {
    "name": "ice-react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fog3211/frontEnd"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint"
    }
  }
}
